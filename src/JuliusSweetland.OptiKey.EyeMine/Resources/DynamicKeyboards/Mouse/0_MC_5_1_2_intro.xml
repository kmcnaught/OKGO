<Keyboard>
   <HideFromKeyboardMenu>True</HideFromKeyboardMenu>
   <SymbolMargin>10</SymbolMargin>
   <BorderThickness>1,30,1,1</BorderThickness>
   <Grid>
      <Rows>1</Rows>
      <Cols>5</Cols>
   </Grid>
   <Keys>
    <DynamicKey Col="0" Row="0">
		 <Symbol>ArrowPointingToLeftIcon</Symbol>
		 <ChangeKeyboard BackReturnsHere="False">0_MC_5_1_1_intro.xml</ChangeKeyboard>
         </DynamicKey>
      <DynamicKey Col="1" Row="0">
         <Label>Walk</Label>
		 <Symbol>MinecraftWalk</Symbol>
         <Text>h</Text>
         </DynamicKey>	  
	  <DynamicKey Col="2" Row="0">
		 <Text>m</Text>
		 <Label>Toggle 
mining</Label>
		 <Symbol>MinecraftMineContinuous</Symbol>
         </DynamicKey>		    
      <DynamicKey Col="3" Row="0">
         <Label>Night\nVision</Label>
		 <Symbol>MinecraftNightVision</Symbol>	 
         <Action>F12</Action>
         </DynamicKey>      
	  <DynamicKey Col="4" Row="0">
        <Symbol>BackIcon</Symbol>
        <Action>BackFromKeyboard</Action>
		</DynamicKey>	 
    </Keys>
</Keyboard>