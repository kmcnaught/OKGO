<Keyboard>
   <Name>Minecraft\nIntro++</Name>
   <SymbolMargin>10</SymbolMargin>
   <BorderThickness>1,30,1,1</BorderThickness>
   <Grid>
      <Rows>1</Rows>
      <Cols>5</Cols>
   </Grid>
   <Keys>
       <DynamicKey Col="0" Row="0">
         <Symbol>MinecraftAutoFly</Symbol>
         <Label>Fly</Label>
         <Text>g</Text>
         </DynamicKey>
	   <DynamicKey Col="1" Row="0">
         <Label>Walk</Label>
		 <Symbol>MinecraftWalk</Symbol>
         <Text>h</Text>
         </DynamicKey>
	  <DynamicKey Col="2" Row="0">
		 <Symbol>MinecraftLookWithCursor</Symbol>	 
		 <Text>y</Text>
         <Label>Look\nwith\nGaze</Label>
         </DynamicKey>
	  <DynamicKey Col="3" Row="0">
		 <Text>r</Text>
		 <Label>Attack</Label>
		 <Symbol>MinecraftAttackContinuous</Symbol>
         </DynamicKey>	
	  <DynamicKey Col="4" Row="0">
		<Symbol>ArrowPointingToRightIcon</Symbol>
		<ChangeKeyboard BackReturnsHere="False">1_MC_5_1_2_advanced.xml</ChangeKeyboard>
		 </DynamicKey>	  
   </Keys>
</Keyboard>