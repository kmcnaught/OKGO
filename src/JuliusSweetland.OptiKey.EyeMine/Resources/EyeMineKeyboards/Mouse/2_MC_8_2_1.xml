         <Text>,</Text>
         </DynamicKey>
      <DynamicKey Col="1" Row="1">
        <Text>.</Text>
        <Label>Fly\nhigher</Label>
        <Symbol>MinecraftFlyUp</Symbol>
        </DynamicKey>
     
     <DynamicKey Col="2" Row="0">
      <Label>Auto-fly</Label>
      <Symbol>MinecraftAutoFly</Symbol>
      <Text>g</Text>
      </DynamicKey>
   <DynamicKey Col="2" Row="1">
      <Label>Auto-jump</Label>
      <Symbol>MinecraftAutoJump</Symbol>
      <Text>j</Text>
      </DynamicKey>
   <DynamicKey Col="3" Row="0" Width="2">
		 <Symbol>MinecraftLookWithCursor</Symbol>	
