   <SymbolMargin>25</SymbolMargin>
   <Grid>
      <Rows>2</Rows>
      <Cols>8</Cols>
   </Grid>
   <Keys>
      <DynamicKey Col="0" Row="0">
         <Label>Toggle\nauto-swim</Label>
         <Symbol>MinecraftSwim</Symbol>
         <Text>v</Text>
         </DynamicKey>	  
      <DynamicKey Col="0" Row="1">	
         <Symbol>ArrowPointingToLeftIcon</Symbol>

         </DynamicKey>
      
      
      <DynamicKey Col="1" Row="0">
         <Label>Fly</Label>
