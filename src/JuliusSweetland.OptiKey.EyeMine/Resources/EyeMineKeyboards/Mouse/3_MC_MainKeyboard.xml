<Keyboard>
   <Name>Minecraft\nAdvanced</Name>
   <SymbolMargin>-5</SymbolMargin>
   <Grid>
      <Rows>7</Rows>
      <Cols>9</Cols>
   </Grid>
   <InitialKeyStates>
     <KeyLockedDown>MouseMagnifier</KeyLockedDown>   
   </InitialKeyStates>
	<Keys>
		<DynamicKey Col="0" Height="1" Row="0" Width="2">
			<Label>Building</Label>
			</DynamicKey>
		<DynamicKey Col="2" Height="1" Row="0" Width="4">
			<Label>Navigating</Label>
			</DynamicKey>
		<DynamicKey Col="6" Height="1" Row="0" Width="1">
			<Label />
			</DynamicKey>
		<DynamicKey Col="7" Height="1" Row="0" Width="1">
			<Label>Mining</Label>
			</DynamicKey>
		<DynamicKey Col="8" Height="1" Row="0" Width="1">
			<Label />
			</DynamicKey>
		<DynamicKey Col="0" Height="2" Row="1" Width="1">
			<Label>Prev\nitem</Label>
			<Symbol>MinecraftPrev</Symbol>
			<Action>NumPad4</Action>
			</DynamicKey>
		<DynamicKey Col="1" Height="2" Row="1" Width="1">
			<Label>Next\nitem</Label>
			<Symbol>MinecraftNext</Symbol>
			<Action>NumPad5</Action>
			</DynamicKey>
		<DynamicKey Col="2" Height="2" Row="1" Width="1">
			<Label>Fly</Label>
			<Symbol>MinecraftFly</Symbol>
			<Text>,</Text>
			</DynamicKey>
        <DynamicKey Col="3" Height="2" Row="1" Width="2">
		 <Symbol>MinecraftLookWithCursor</Symbol>	
		 <Text>y</Text>
         <Label>Look\nwith\nGaze</Label>
        </DynamicKey> 	 
		<DynamicKey Col="5" Height="2" Row="1" Width="1">
			<Label>Toggle\nauto-jump</Label>
			<Symbol>MinecraftAutoJump</Symbol>
			<Text>j</Text>
			</DynamicKey>
		<DynamicKey Col="6" Height="2" Row="1" Width="1">
			<Label>Toggle\nattack</Label>
			<Symbol>MinecraftAttackContinuous</Symbol>
			<Text>r</Text>
			</DynamicKey>
		<DynamicKey Col="7" Height="2" Row="1" Width="1">
			<Label>Mine\none block</Label>
			<Symbol>MinecraftMineOnce</Symbol>
			<Text>n</Text>
			</DynamicKey>
		<DynamicKey Col="8" Height="2" Row="1" Width="1">
			<Label>Esc</Label>
			<Symbol>MinecraftEscButton</Symbol>
			<Action>Escape</Action>
			</DynamicKey>
		<DynamicKey Col="0" Height="2" Row="3" Width="1">
			<Label>Use\nItem</Label>
			<Symbol>MouseRightClickIcon</Symbol>
			<Action>NumPad0</Action>
			</DynamicKey>
		<DynamicKey Col="1" Height="2" Row="3" Width="1">
		    <Label>Dwell\nbuilding</Label>
			<Symbol>DwellBuild</Symbol>
			<Action>NumPad3</Action>
			</DynamicKey>
		<DynamicKey Col="2" Height="2" Row="3" Width="1">
			<Label>Fly\nUp</Label>
			<Symbol>MinecraftFlyUp</Symbol>
			<Text>.</Text>
			</DynamicKey>
		<DynamicKey Col="3" Height="2" Row="3" Width="1">
			<Label>Walk</Label>
			<Symbol>MinecraftWalk</Symbol>
			<Text>h</Text>
			</DynamicKey>
		<DynamicKey Col="4" Height="2" Row="3" Width="1">
			<Label>Careful\nwalk</Label>
			<Symbol>MinecraftMoveLegacy</Symbol>
			<Text>b</Text>
			</DynamicKey>
		<DynamicKey Col="5" Height="2" Row="3" Width="1">
			<Label>Sneak</Label>
			<Symbol>MinecraftSneak</Symbol>
			<Text>z</Text>
			</DynamicKey>
		<DynamicKey Col="6" Height="2" Row="3" Width="1">
			<Label>Swap mine\nand place</Label>
			<Symbol>MinecraftSwapMinePlace</Symbol>
			<Action>F10</Action>
			</DynamicKey>
		<DynamicKey Col="7" Height="2" Row="3" Width="1">
			<Label>Toggle\nmining</Label>
			<Symbol>MinecraftMineContinuous</Symbol>
			<Text>m</Text>
			</DynamicKey>
		<DynamicKey Col="8" Height="2" Row="3" Width="1">
			<Label>Inventory\netc...</Label>
			<ChangeKeyboard BackReturnsHere="True">3_MC_InventoryKeyboard</ChangeKeyboard>
			</DynamicKey>
		<DynamicKey Col="0" Height="2" Row="5" Width="1">
			<Label>Auto\nPillar</Label>
			<Symbol>MinecraftPlaceBlock</Symbol>
			<Text>0</Text>
			</DynamicKey>
		<DynamicKey Col="1" Height="2" Row="5" Width="1">
			<Label>Pick\nBlock</Label>
			<Symbol>MinecraftPickBlock</Symbol>
			<Action>NumPad2</Action>
			</DynamicKey>
		<DynamicKey Col="2" Height="2" Row="5" Width="1">
			<Label>Auto\nFly</Label>
			<Symbol>MinecraftAutoFly</Symbol>
			<Text>g</Text>
			</DynamicKey>
		<DynamicKey Col="3" Height="2" Row="5" Width="1">
			<Label>Night\nVision</Label>
			<Symbol>MinecraftNightVision</Symbol>
			<Action>F12</Action>
			</DynamicKey>
		<DynamicKey Col="4" Height="2" Row="5" Width="1">
			<Label>Chat...</Label>
			<Symbol>ChatIcon</Symbol>
			<ChangeKeyboard BackReturnsHere="True">3_MC_ChatKeyboard</ChangeKeyboard>
			</DynamicKey>
		<DynamicKey Col="5" Height="2" Row="5" Width="1">
			<Label>Toggle\nSwim</Label>
			<Symbol>MinecraftSwim</Symbol>
			<Text>v</Text>
			</DynamicKey>
		<DynamicKey Col="6" Height="2" Row="5" Width="1">
			<Label>Left\nclick</Label>
			<Symbol>MouseLeftClickWithMouseIcon</Symbol>
			<Action>MouseMoveAndLeftClick</Action>
			</DynamicKey>
		<DynamicKey Col="7" Height="2" Row="5" Width="1">
			<Label>Ride/\nDismount</Label>
			<Action>F15</Action>
			</DynamicKey>
		<DynamicKey Col="8" Height="2" Row="5" Width="1">
			<Label>Back</Label>
			<Symbol>BackIcon</Symbol>
			<Action>BackFromKeyboard</Action>
			</DynamicKey>
	</Keys>
</Keyboard>