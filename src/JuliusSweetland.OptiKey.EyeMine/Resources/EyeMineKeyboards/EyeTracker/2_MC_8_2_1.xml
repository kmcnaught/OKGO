<Keyboard>
  <BorderThickness>1,30,1,1</BorderThickness>
  <SymbolMargin>25</SymbolMargin>
  <Name>Minecraft\nIntermediate</Name>
  <Grid>
    <Rows>2</Rows>
    <Cols>8</Cols>
  </Grid>
  <Content>
    <DynamicKey>
      <Label>Toggle\nauto-swim</Label>
      <Symbol>MinecraftSwim</Symbol>
      <Text>v</Text>
    </DynamicKey>
    <DynamicKey>
      <Label>Fly</Label>
      <Symbol>MinecraftFly</Symbol>
      <Text>,</Text>
    </DynamicKey>
    <DynamicKey>
      <Label>Auto-fly</Label>
      <Symbol>MinecraftAutoFly</Symbol>
      <Text>g</Text>
    </DynamicKey>
    <DynamicKey Width="2">
      <Symbol>MinecraftLookWithCursor</Symbol>
      <Action>MouseMagneticCursor</Action>
      <Label>Look\nwith\nGaze</Label>
      <SharedSizeGroup>Look</SharedSizeGroup>
    </DynamicKey>
    <DynamicKey>
      <Label>Night\nvision</Label>
      <Symbol>MinecraftNightVision</Symbol>
      <Action>F12</Action>
    </DynamicKey>
    <DynamicKey>
      <Label>Mine x1</Label>
      <Symbol>MinecraftMineOnce</Symbol>
