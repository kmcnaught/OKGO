    </DynamicKey>
    <DynamicKey>
      <Label>Toggle\nattack</Label>
      <Symbol>MinecraftAttackContinuous</Symbol>
      <Text>r</Text>
    </DynamicKey>
    <DynamicKey>
      <Symbol>ArrowPointingToLeftIcon</Symbol>
    </DynamicKey>
    <DynamicKey>
      <Text>.</Text>
      <Label>Fly\nhigher</Label>
      <Symbol>MinecraftFlyUp</Symbol>
    </DynamicKey>
    <DynamicKey>
      <Label>Auto-jump</Label>
      <Symbol>MinecraftAutoJump</Symbol>
      <Text>j</Text>
    </DynamicKey>
    <DynamicKey Width="2">
      <Label>Toggle\nwalking</Label>
      <Symbol>MinecraftWalk</Symbol>
      <Text>h</Text>
      <SharedSizeGroup>Walk</SharedSizeGroup>
    </DynamicKey>
    <DynamicKey>
      <Label>Ride/\nDismount</Label>
      <Action>F15</Action>
    </DynamicKey>
    <DynamicKey>
      <Label>Toggle\nmining</Label>
      <Symbol>MinecraftMineContinuous</Symbol>
      <Text>m</Text>
    </DynamicKey>
    <DynamicKey>
      <ChangeKeyboard BackReturnsHere="False">2_MC_8_2_2.xml</ChangeKeyboard>
      <Symbol>ArrowPointingToRightIcon</Symbol>
    </DynamicKey>
  </Content>
