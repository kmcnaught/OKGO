<Keyboard>
    <HideFromKeyboardMenu>True</HideFromKeyboardMenu>
	<WindowState>Floating</WindowState>
	<Position>Bottom</Position>
	<Width>100%</Width>
	<Height>-80</Height>
	<VerticalOffset>-80</VerticalOffset>
    <SymbolMargin>10</SymbolMargin>
    <BorderThickness>1,1,1,1</BorderThickness>
	<BackgroundColor>Transparent</BackgroundColor>
	<BorderColor>Transparent</BorderColor>
    <Grid>
        <Rows>14</Rows>
        <Cols>16</Cols>
    </Grid>
    <Content>

        <DynamicKey Col="15" Height="2"><Label>Back</Label><Symbol>BackIcon</Symbol><Action>BackFromKeyboard</Action></DynamicKey>
        <DynamicKey Col="15" Height="3"><Action>MouseMoveAndLeftClick</Action><Label>Left Click</Label><Symbol>MouseLeftClickIcon</Symbol></DynamicKey>
        <DynamicKey Col="15" Height="2"><Action>MouseMoveAndRightClick</Action><Label>Right Click</Label><Symbol>MouseRightClickIcon</Symbol></DynamicKey>
        <DynamicKey Col="15" Height="2"><Action>LookToScrollActive</Action><Label>Scroll</Label><Symbol>LookToScrollActiveIcon</Symbol></DynamicKey>
        <DynamicKey Col="15" Height="1"><Action>Sleep</Action><Label>Sleep</Label><Symbol>SleepIcon</Symbol></DynamicKey>
		
        <DynamicKey Row="10"><Action>Escape</Action><Label>Esc</Label></DynamicKey>
        <DynamicKey><Action>F1</Action><Label>F1</Label></DynamicKey>
        <DynamicKey><Action>F2</Action><Label>F2</Label></DynamicKey>
        <DynamicKey><Action>F3</Action><Label>F3</Label></DynamicKey>
        <DynamicKey><Action>F4</Action><Label>F4</Label></DynamicKey>
        <DynamicKey><Action>F5</Action><Label>F5</Label></DynamicKey>
        <DynamicKey><Action>F6</Action><Label>F6</Label></DynamicKey>
        <DynamicKey><Action>F7</Action><Label>F7</Label></DynamicKey>
        <DynamicKey><Action>F8</Action><Label>F8</Label></DynamicKey>
        <DynamicKey><Action>F9</Action><Label>F9</Label></DynamicKey>
        <DynamicKey><Action>F10</Action><Label>F10</Label></DynamicKey>
        <DynamicKey BackgroundColor="white" ForegroundColor="black" KeyDisabledBackground="gray"><Label>Back</Label><Symbol>BackIcon</Symbol><ChangeKeyboard BackReturnsHere="false">FullQwerty</ChangeKeyboard></DynamicKey>
        <DynamicKey><Action>Delete</Action><Label>Del</Label></DynamicKey>
        <DynamicKey><Action>PgUp</Action><Label>Pg Up</Label></DynamicKey>
        <DynamicKey><Action>BackOne</Action><Label>Backspace</Label><Symbol>BackOneIcon</Symbol></DynamicKey>
        <DynamicKey><Action>Home</Action><Label>Home</Label></DynamicKey>
        
        <DynamicKey Row="11"><Text>&#9;</Text><Label>Tab</Label><Symbol>TabIcon</Symbol></DynamicKey>
        <DynamicKey><Action>F11</Action><Label>F11</Label></DynamicKey>
        <DynamicKey><Action>F12</Action><Label>F12</Label></DynamicKey>
        <DynamicKey Width="2" KeyDisabledBackground="#575757"></DynamicKey>
        <DynamicKey><Action>MouseLeftDoubleClick</Action><Label>Double Click</Label><Symbol>MouseLeftDoubleClickIcon</Symbol></DynamicKey>
        <DynamicKey><Action>MouseLeftClick</Action><Label>Left Click</Label><Symbol>MouseLeftClickIcon</Symbol></DynamicKey>
        <DynamicKey><Action>MouseMoveToTop</Action><Symbol>ArrowPointingToTopIcon</Symbol></DynamicKey>
        <DynamicKey><Action>MouseRightClick</Action><Label>Right Click</Label><Symbol>MouseRightClickIcon</Symbol></DynamicKey>
        <DynamicKey Col="9"><Text>~</Text><Label>~</Label></DynamicKey>
        <DynamicKey><Text>{</Text><Label>{</Label></DynamicKey>
        <DynamicKey><Text>}</Text><Label>}</Label></DynamicKey>
        <DynamicKey><Text>|</Text><Label>|</Label></DynamicKey>		
		<DynamicKey Col="13"><Action>PgDn</Action><Label>Pg Dn</Label></DynamicKey>
        <DynamicKey><Action>ArrowUp</Action><Symbol>UpArrowKeyIcon</Symbol></DynamicKey>
        <DynamicKey><Action>End</Action><Label>End</Label></DynamicKey>
        
        <DynamicKey Row="12"><Action>LeftShift</Action><Label>Shift</Label><Symbol>ShiftIcon</Symbol></DynamicKey>
        <DynamicKey Width="5" KeyDisabledBackground="#575757"><Label></Label></DynamicKey>	
        <DynamicKey><Action>MouseMoveToLeft</Action><Symbol>ArrowPointingToLeftIcon</Symbol></DynamicKey>
        <DynamicKey><Action>MouseMoveTo</Action><Label>Move Mouse</Label><Symbol>MouseMoveToIcon</Symbol></DynamicKey>
        <DynamicKey><Action>MouseMoveToRight</Action><Symbol>ArrowPointingToRightIcon</Symbol></DynamicKey>
        <DynamicKey Col="9"><Text>`</Text><Label>`</Label></DynamicKey>
        <DynamicKey><Text>[</Text><Label>[</Label></DynamicKey>		
        <DynamicKey><Text>]</Text><Label>]</Label></DynamicKey>
        <DynamicKey><Text>\</Text><Label>\</Label></DynamicKey>
        <DynamicKey Col="13"><Action>ArrowLeft</Action><Symbol>LeftArrowKeyIcon</Symbol></DynamicKey>
        <DynamicKey><Action>ArrowDown</Action><Symbol>DownArrowKeyIcon</Symbol></DynamicKey>
        <DynamicKey><Action>ArrowRight</Action><Symbol>RightArrowKeyIcon</Symbol></DynamicKey>
        
        <DynamicKey Row="13"><Action>LeftCtrl</Action><Label>Ctrl</Label></DynamicKey>
        <DynamicKey><Action>LeftAlt</Action><Label>Alt</Label></DynamicKey>
		<DynamicKey Width="3" KeyDisabledBackground="#575757"><Label></Label></DynamicKey>
        <DynamicKey><Action>LookToScrollActive</Action><Label>Scroll</Label><Symbol>LookToScrollActiveIcon</Symbol></DynamicKey>
		<DynamicKey Width="1" KeyDisabledBackground="#575757"><Label></Label></DynamicKey>
        <DynamicKey><Action>MouseMoveToBottom</Action><Symbol>ArrowPointingToBottomIcon</Symbol></DynamicKey>
		<DynamicKey Width="1" KeyDisabledBackground="#575757"><Label></Label></DynamicKey>
        <DynamicKey><Action>MouseMagnifier</Action><Label>Magnifier</Label><Symbol>MouseMagnifierIcon</Symbol></DynamicKey>
        <DynamicKey><Label>Sleep</Label><Symbol>SleepIcon</Symbol><Action>Sleep</Action></DynamicKey>
        <DynamicKey><Label>Menu</Label><Symbol>MenuIcon</Symbol><ChangeKeyboard BackReturnsHere="false">Menu</ChangeKeyboard></DynamicKey>    
        <DynamicKey><Action>Minimise</Action><Label>Hide</Label><Symbol>ExpandDockIcon</Symbol></DynamicKey>    
        <DynamicKey Col="13"><Action>LeftAlt</Action><Label>Alt</Label></DynamicKey>
        <DynamicKey><Action>LeftCtrl</Action><Label>Ctrl</Label></DynamicKey>
        <DynamicKey><Text>&#13;</Text><Label>Enter</Label><Symbol>EnterIcon</Symbol></DynamicKey>
    </Content>
</Keyboard>