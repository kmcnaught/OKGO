<Keyboard>
    <Name>Demo_Bottom</Name>
    <HideFromKeyboardMenu>False</HideFromKeyboardMenu>
	<PersistNewState></PersistNewState>  
	<WindowState>Floating</WindowState>
	<DockSize>Full</DockSize>
	<Position>BottomLeft</Position> 
	<Width>90%</Width>
	<Height>40%</Height>  
	<HorizontalOffset></HorizontalOffset>
	<VerticalOffset>-80</VerticalOffset>
    <SymbolMargin>4</SymbolMargin>
    <BorderThickness>1,1,1,1</BorderThickness>
	<BackgroundColor></BackgroundColor>
	<BorderColor></BorderColor>
    <Grid>
        <Rows>6</Rows>
        <Cols>12</Cols>
    </Grid>
	<KeyGroup Name="SG1" BackgroundColor="darkred" KeyDisabledOpacity=".5" CompletionTimes="1000,750,500,250" />
	<KeyGroup Name="SG2" BackgroundColor="darkgray" KeyDisabledOpacity=".5"  CompletionTimes="600" />
	<KeyGroup Name="SG3" BackgroundColor="darkblue" KeyDisabledOpacity=".5" CompletionTimes="1000,500" />
	<Content>
        <DynamicKey Row="0" Col="0">
			<Label>Sleep</Label>
			<Symbol>SleepIcon</Symbol>
			<Action>Sleep</Action>
		</DynamicKey>
		<DynamicKey Row="0" Col="2">
			<Action>PreviousSuggestions</Action>
			<Symbol>SuggestionLeftIcon</Symbol>
		</DynamicKey>
		<SuggestionRow Width="6" />
		<DynamicKey Row="0">
			<Action>NextSuggestions</Action>
			<Symbol>SuggestionRightIcon</Symbol>
		</DynamicKey>
		<DynamicKey Row="1" Col="2">
			<Action>ClearScratchpad</Action>
			<Label>Clear</Label>
			<Symbol>ClearIcon</Symbol>
		</DynamicKey>
		<Scratchpad Width="6" />
		<DynamicKey>
			<Action>CopyAllFromScratchpad</Action>
			<Label>Copy</Label>
			<Symbol>CopyAllIcon</Symbol>
		</DynamicKey>
		<DynamicKey Row="1" Col="11">
			<Label>Back</Label>
			<Symbol>BackIcon</Symbol>
			<Action>BackFromKeyboard</Action>
		</DynamicKey>		
        <DynamicKey Row="0" Col="11">
			<Label>Menu</Label>
			<Symbol>MenuIcon</Symbol>
			<ChangeKeyboard>Menu</ChangeKeyboard>
		</DynamicKey>

        <DynamicKey Row="2"><KeyGroup>SG1</KeyGroup><Action>Escape</Action><Label>Esc</Label></DynamicKey>
        <DynamicKey><KeyGroup>SG2</KeyGroup><Text>1</Text><Label>1</Label><ShiftDownLabel>!</ShiftDownLabel></DynamicKey>
        <DynamicKey><KeyGroup>SG2</KeyGroup><Text>2</Text><Label>2</Label><ShiftDownLabel>@</ShiftDownLabel></DynamicKey>
        <DynamicKey><KeyGroup>SG2</KeyGroup><Text>3</Text><Label>3</Label><ShiftDownLabel>#</ShiftDownLabel></DynamicKey>
        <DynamicKey><KeyGroup>SG2</KeyGroup><Text>4</Text><Label>4</Label><ShiftDownLabel>$</ShiftDownLabel></DynamicKey>
        <DynamicKey><KeyGroup>SG2</KeyGroup><Text>5</Text><Label>5</Label><ShiftDownLabel>%</ShiftDownLabel></DynamicKey>
        <DynamicKey><KeyGroup>SG2</KeyGroup><Text>6</Text><Label>6</Label><ShiftDownLabel>^</ShiftDownLabel></DynamicKey>
        <DynamicKey><KeyGroup>SG2</KeyGroup><Text>7</Text><Label>7</Label><ShiftDownLabel>&amp;</ShiftDownLabel></DynamicKey>
        <DynamicKey><KeyGroup>SG2</KeyGroup><Text>8</Text><Label>8</Label><ShiftDownLabel>*</ShiftDownLabel></DynamicKey>
        <DynamicKey><KeyGroup>SG2</KeyGroup><Text>9</Text><Label>9</Label><ShiftDownLabel>(</ShiftDownLabel></DynamicKey>
        <DynamicKey><KeyGroup>SG2</KeyGroup><Text>0</Text><Label>0</Label><ShiftDownLabel>)</ShiftDownLabel></DynamicKey>
        <DynamicKey><KeyGroup>SG1</KeyGroup><Action>Delete</Action><Label>Del</Label></DynamicKey>	
        
        <DynamicKey><KeyGroup>SG1</KeyGroup><Text>&#9;</Text><Label>Tab</Label><Symbol>TabIcon</Symbol></DynamicKey>
        <DynamicKey><Text>q</Text><Label>q</Label><ShiftDownLabel>Q</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>w</Text><Label>w</Label><ShiftDownLabel>W</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>e</Text><Label>e</Label><ShiftDownLabel>E</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>r</Text><Label>r</Label><ShiftDownLabel>R</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>t</Text><Label>t</Label><ShiftDownLabel>T</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>y</Text><Label>y</Label><ShiftDownLabel>Y</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>u</Text><Label>u</Label><ShiftDownLabel>U</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>i</Text><Label>i</Label><ShiftDownLabel>I</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>o</Text><Label>o</Label><ShiftDownLabel>O</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>p</Text><Label>p</Label><ShiftDownLabel>P</ShiftDownLabel></DynamicKey>
        <DynamicKey><KeyGroup>SG1</KeyGroup><Action>BackOne</Action><Label>Backspace</Label><Symbol>BackOneIcon</Symbol></DynamicKey>
                
        <DynamicKey><KeyGroup>SG3</KeyGroup><Action>LeftShift</Action><Label>Shift</Label><Symbol>ShiftIcon</Symbol></DynamicKey>
		<DynamicKey><Text>a</Text><Label>a</Label><ShiftDownLabel>A</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>s</Text><Label>s</Label><ShiftDownLabel>S</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>d</Text><Label>d</Label><ShiftDownLabel>D</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>f</Text><Label>f</Label><ShiftDownLabel>F</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>g</Text><Label>g</Label><ShiftDownLabel>G</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>h</Text><Label>h</Label><ShiftDownLabel>H</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>j</Text><Label>j</Label><ShiftDownLabel>J</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>k</Text><Label>k</Label><ShiftDownLabel>K</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>l</Text><Label>l</Label><ShiftDownLabel>L</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>'</Text><Label>'</Label><ShiftDownLabel>"</ShiftDownLabel></DynamicKey>
        <DynamicKey><KeyGroup>SG1</KeyGroup><Text>&#13;</Text><Label>Enter</Label><Symbol>EnterIcon</Symbol></DynamicKey>
        
        <DynamicKey><KeyGroup>SG3</KeyGroup><Action>LeftCtrl</Action><Label>Ctrl</Label></DynamicKey>
        <DynamicKey><KeyGroup>SG3</KeyGroup><Action>LeftAlt</Action><Label>Alt</Label></DynamicKey>
        <DynamicKey><Text>z</Text><Label>z</Label><ShiftDownLabel>Z</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>x</Text><Label>x</Label><ShiftDownLabel>X</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>c</Text><Label>c</Label><ShiftDownLabel>C</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>v</Text><Label>v</Label><ShiftDownLabel>V</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>&#32;</Text><Label>Space</Label><Symbol>SpaceIcon</Symbol></DynamicKey>
        <DynamicKey><Text>b</Text><Label>b</Label><ShiftDownLabel>B</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>n</Text><Label>n</Label><ShiftDownLabel>N</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>m</Text><Label>m</Label><ShiftDownLabel>M</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>,</Text><Label>,</Label><ShiftDownLabel>&lt;</ShiftDownLabel></DynamicKey>
        <DynamicKey><Text>.</Text><Label>.</Label><ShiftDownLabel>&gt;</ShiftDownLabel></DynamicKey>		
	</Content>
</Keyboard>